import assert from "assert";

function nonEmptyLines(text: string): number {
    // your code here
    return 0;
}

console.log('Example:');
console.log(nonEmptyLines('one simple line\n'));

// These "asserts" are used for self-checking
assert.equal(nonEmptyLines('one simple line\n'), 1);
assert.equal(nonEmptyLines(''), 0);
assert.equal(nonEmptyLines('\nonly one line\n            '), 1);
assert.equal(nonEmptyLines(`
Lorem ipsum dolor sit amet,

consectetur adipiscing elit
Nam odio nisi, aliquam
            `), 3);

